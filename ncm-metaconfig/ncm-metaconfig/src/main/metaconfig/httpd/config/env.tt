[%- FOREACH sif IN desc.if %]
setenvif [% sif.attribute %] [% sif.quotes %][% sif.regex %][% sif.quotes %] [% sif.variables.join(' ') %]
[%- END %]
[%- FOREACH pair IN desc.set.pairs %]
setenv [% pair.key %] [% pair.value %]
[%- END %]
[%- FOREACH s IN desc.unset %]
unsetenv [% s.name %]
[% END %]
[% IF desc.exists('pass') %]
passenv [% desc.pass.join(' ') %]]
[% END %]
