[%- booleans = ['engine', 'compression', 'sessiontickets',
                'renegotiation', 'requiresafenegotiation', 'ocsp',
                ] -%]
[%- list = ["options"] -%]
[%- colon_list = ["ciphersuite"] -%]
[%- list_of_val = ["cryptodevice"] -%]
[%- list_of_list = ["randomseed"] -%]
[%- quotes = ["nickname"] -%]
[%- FOREACH pair IN desc.pairs %]
[%-     SWITCH pair.key -%]
[%-         CASE "ciphersuite" -%]
[%              ssl_nss %][% pair.key %] [% pair.value.join(ssl_nss == 'nss' ? ',' : ':') %]
[%-         CASE "protocol" -%]
[%              ssl_nss %][% pair.key %] [% pair.value.join(ssl_nss == 'nss' ? ',' : ' ') %]
[%-         CASE "mutex" -%]
[%              (_httpdversion.0 >=2 && _httpdversion.1 >=4) ? '' : ssl_nss %][% pair.key %] [% pair.value %]
[%-         CASE "requiressl" -%]
[%              ssl_nss %][% pair.key %]
[%-         CASE list -%]
[%              ssl_nss %][% pair.key %] [% pair.value.join(' ') %]
[%-         CASE quotes -%]
[%              ssl_nss %][% pair.key %] "[% pair.value %]"
[%-         CASE colon_list -%]
[%              ssl_nss %][% pair.key %] [% pair.value.join(':') %]
[%-         CASE list_of_val -%]
[%              FOREACH val IN pair.value %]
[%                  ssl_nss %][% pair.key %] [% val %]
[%              END %]
[%-         CASE list_of_list -%]
[%              FOREACH li IN pair.value %]
[%                  ssl_nss %][% pair.key %] [% li.join(' ') %]
[%              END %]
[%-         CASE booleans -%]
[%              ssl_nss %][% pair.key %] [% pair.value ? "on" : "off" %]
[%-         CASE -%]
[%              ssl_nss %][% pair.key %] [% pair.value %]
[%-      END %]
[%  END %]
