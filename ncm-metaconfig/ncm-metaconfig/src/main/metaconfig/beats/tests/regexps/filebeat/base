Filebeat basic value based test
---
/etc/filebeat/filebeat.yml
---
^---$
^filebeat:$
^  prospectors:$
^  - fields:$
^      type: special$
^    paths:$
^    - /path/1$
^    - /path/2/\*$
^  - fields:$
^      typeb: specialb$
^    paths:$
^    - /path/1b$
^    - /path/2b/\*$
^output:$
^  console:$
^    pretty: true$
^  logstash:$
^    hosts:$
^    - localhost:1234$
^    - otherhost:5678$
^    loadbalance: true$
