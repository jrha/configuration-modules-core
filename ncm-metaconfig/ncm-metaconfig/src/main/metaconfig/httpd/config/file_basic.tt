[%- IF desc.exists('options') -%]
options [% desc.options.join(' ') %]
[% END -%]
[%- IF desc.exists('enablesendfile') -%]
enablesendfile [% desc.enablesendfile ? "on" : "off" %]
[% END -%]
[%- to_process = ['access', 'lang', 'ssl', 'nss', 'auth', 'davrods', 'authz', 'kerberos', 'shibboleth', 'gssapi'] -%]
[%- FOREACH p IN  to_process -%]
[%-      IF desc.exists(p) -%]
[%          INCLUDE "metaconfig/httpd/config/${p}.tt" desc=desc.$p %]
[%      END -%]
[%- END %]
